<% include partials/head %>
    <% include partials/header %>

    <ul id="quotes">

    <% quotes.forEach(function(quote) { %>

      <li>
        <blockquote>

          <p><%= quote.text %></p>
          <footer><%= quote.author %></footer>

        </blockquote>
      </li>

    <% }); %>

    <nav>
      <%- link_to('Add a New Quote', 'hello') %>
    </nav>

    </ul>

<% include partials/footer %>